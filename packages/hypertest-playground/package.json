{
  "name": "@hypertest/hypertest-playground",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start:core": "node --env-file=.env ./dist/src/core.js",
    "start:runner": "node --env-file=.env ./dist/src/runner.js",
    "cy:open": "cypress open",
    "cy:run": "cypress run --headless",
    "test:run": "node ./dist/scripts/run.js",
    "lint": "biome lint"
  },
  "dependencies": {
    "@hypertest/hypertest-core": "0.x.x",
    "@hypertest/hypertest-plugin-playwright": "0.x.x",
    "@hypertest/hypertest-runner-playwright": "0.x.x"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@playwright/test": "^1.49.0",
    "@types/node": "^22.9.3",
    "typescript": "^5.7.2"
  }
}
